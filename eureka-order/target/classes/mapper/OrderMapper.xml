<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lee.order.dao.OrderDao">


	<select id="selectOrderById" parameterType="java.lang.Long" resultType="com.lee.order.bean.Order">
		select * from
		t_order t
		where
			t.id = #{id}
	</select>
</mapper>